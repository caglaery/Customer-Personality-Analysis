                                                        cltv_df["T"]).sort_values(ascending=False).head(10)






bgf.conditional_expected_number_of_purchases_up_to_time(52,cltv_df["Frequency"],
                                                        cltv_df["Recency"],
                                                        cltv_df["T"]).sort_values(ascending=False).head(10)





cltv_df["exp_num_sale_3m"]=bgf.predict(4*3,cltv_df["Frequency"],
                                           cltv_df["Recency"],
