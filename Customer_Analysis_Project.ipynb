def outlier_thresholds(dataframe,variable):
        q1=dataframe[variable].quantile(0.25)
        q3=dataframe[variable].quantile(0.75)
        iqr=q3-q1
        lower=q1-1.5*iqr
        upper=q3+1.5*iqr
        return lower,upper




